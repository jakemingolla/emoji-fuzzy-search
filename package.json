{
  "name": "emoji-fuzzy-search",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "prestart": "docker-compose up -d && bun run database:migrations:up-to-latest && bun run database:generate-types",
    "start": "bun --watch run src/index.ts",
    "stop": "docker-compose down",
    "restart": "bun run stop && bun run start",
    "database:migrations:create": "./scripts/create-migration.sh",
    "database:migrations:up-to-latest": "bun run ./scripts/run-migrations.ts -- --up-to-latest",
    "database:migrations:up": "bun run ./scripts/run-migrations.ts -- --up",
    "database:migrations:down": "bun run ./scripts/run-migrations.ts -- --down",
    "database:generate-types": "bunx kysely-codegen && cp ./node_modules/kysely-codegen/dist/db.d.ts ./src/types/database.d.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/keypress": "^2.0.30",
    "@types/react": "^18.3.3",
    "kysely-codegen": "^0.15.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.0.14",
    "elysia": "^1.0.22",
    "ink": "^5.0.1",
    "keypress": "^0.2.1",
    "kysely": "^0.27.3",
    "pg": "^8.11.5",
    "react": "^18.3.1"
  }
}
